{
  "$schema": "http://json.schemastore.org/template",
  "author": "고형호",
  "classifications": [
    "VOC",
    "DDD",
    "Architecture"
  ],
  "name": "DDD Architecture Command",
  "description": "Creates a new DDD Architecture Command",
  "identity": "DDD.Architecture.Command.CSharp",
  "groupIdentity": "DDD.Architecture.Command",
  "shortName": "ddd-arch-command",
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "sourceName": "CommandName",
  "preferNameDirectory": false,
  "sources": [
    {
      "source": "./",
      "target": "./"
    }
  ],
  "symbols": {
    "hostName": {
      "type": "parameter",
      "defaultValue": "HostName",
      "replaces": "HostName",
      "isRequired": true,
      "description": "The name of the host (used for namespace prefix). e.g. Hello"
    },
    "entityName": {
      "type": "parameter",
      "defaultValue": "EntityName",
      "replaces": "EntityName",
      "fileRename": "EntityName",
      "isRequired": true,
      "description": "The name of the entity. e.g. Customer"
    }
  }
}
